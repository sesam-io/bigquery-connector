{
  "_id": "bq_{{@ system @}}",
  "type": "system:microservice",
  "docker": {
    "environment": {
      "CONFIG": {
        "bigquery_table_prefix": "sesam.demo",
        "google_application_credentials": "$SECRET(jwt)",
        "jwt_token": "$SECRET(service_jwt)",
        "multithreaded": true,
        "node_url": "$SECRET(service_url)"
      }
    },
    "image": "sesamcommunity/google-bigquery-sink:latest",
    "memory": 128,
    "port": 5000
  },
  "read_timeout": 7200,
  "use_https": false,
  "verify_ssl": false
}
