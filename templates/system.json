{
  "_id": "bq_{{@ system @}}",
  "type": "system:microservice",
  "docker": {
    "environment": {
      "CONFIG": {
        "bigquery_table_prefix": "bigquery-microservice.test",
        "google_user_credentials": {
          "token": "$SECRET(oauth_access_token)",
          "refresh_token": "$SECRET(oauth_refresh_token)",
          "client_id": "$SECRET(oauth_client_id)",
          "client_secret": "$SECRET(oauth_client_secret)",
          "project": "bigquery-microservice"
        },
        "jwt_token": "$SECRET(service_jwt)",
        "multithreaded": true,
        "node_url": "$SECRET(service_url)"
      }
    },
    "image": "sesamcommunity/google-bigquery-sink:development",
    "memory": 128,
    "port": 5000
  },
  "read_timeout": 7200,
  "use_https": false,
  "verify_ssl": false
}
