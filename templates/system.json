{
  "_id": "bq_{{@ system @}}",
  "type": "system:microservice",
  "docker": {
    "environment": {
      "GOOGLE_APPLICATION_CREDENTIALS": "$SECRET(bigquery-credentials)",
      "JWT_TOKEN": "$SECRET(bigquery-ms-jwt)",
      "MULTITHREADED": "false",
      "NODE_URL": "<todo>"
    },
    "image": "sesam/sesam-bigquery:latest",
    "memory": 128,
    "port": 5000
  },
  "read_timeout": 7200,
  "use_https": false,
  "verify_ssl": false
}